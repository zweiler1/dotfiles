{
	"id": "noctalia-supergfxctl",
	"name": "SuperGFX Control",
	"version": "0.0.3",
	"minNoctaliaVersion": "3.8.2",
	"author": "cod3d <cod3ddot@proton.me>",
	"license": "MIT",
	"repository": "https://github.com/cod3ddot/noctalia-supergfxctl",
	"description": "GPU control with supergfxctl for noctalia.",
	"tags": ["Bar", "Panel", "System", "Gaming"],
	"entryPoints": {
		"main": "src/Main.qml",
		"barWidget": "src/Bar.qml",
		"controlCenterWidget": "src/ControlCenter.qml",
		"settings": "src/Settings.qml",
		"panel": "src/Panel.qml"
	},
	"dependencies": {
		"plugins": []
	},
	"metadata": {
		"defaultSettings": {
			"debug": false,
			"supergfxctl": {
				"patchPending": true,
				"polling": false,
				"pollingInterval": 3000
			},
			"rogcc": {
				"listenToNotifications": false
			}
		}
	}
}
